    "jest": "^27.4.7",
    "process": "^0.11.10",
    "sinon": "^14.0.0",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.4",
    "webassembly-loader": "^1.1.0",
    "webpack": "^5.73.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.10.0",
    "webpack-dev-server": "^4.9.2"
  },
  "dependencies": {
    "@ethersproject/bytes": "^5.6.1",
    "@safeheron/mpcsnap-types": "workspace:^",
    "@safeheron/two-party-mpc-adapter": "workspace:^",
    "aes-js": "^3.1.2",
    "bn.js": "^5.2.1",
