{
  "name": "@safeheron/mpcsnap",
  "version": "1.0.0",
  "description": "MetaMask MPCSnap",
  "main": "dist/index.ts",
  "keywords": [
    "Safeheron",
    "MPC",
    "MetaMask",
    "MetaMask Flask",
    "Snap",
    "Keystone",
    "ETH"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Safeheron/mpcsnap"
  },
  "author": "Yann<yanjie002@gmail.com>",
  "files": [
    "dist/",
    "snap.manifest.json"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "prebuild": "yarn run clean",
    "build": "webpack --progress --config webpack.config.ts",
    "manifest": "mm-snap manifest --fix",
    "eval": "mm-snap eval -b dist/mpcSnapBundle.js --verboseErrors",
    "serve": "yarn run clean && concurrently \"webpack --watch\" \"mm-snap serve\"",
    "test": "jest ./test/**/*.test.ts",
    "cover": "jest ./test/**/*.test.ts --coverage",
    "lint": "eslint --fix --ext .ts,.tsx,.js src/"
  },
  "devDependencies": {
    "@babel/core": "^7.18.5",
    "@babel/preset-env": "^7.18.2",
    "@babel/preset-typescript": "^7.17.12",
    "@babel/register": "^7.17.7",
    "@babel/runtime": "^7.18.6",
    "@metamask/snap-types": "0.18.1",
    "@metamask/snaps-cli": "0.18.1",
    "@metamask/snaps-webpack-plugin": "0.18.1",
    "@types/aes-js": "^3.1.1",
    "@types/crypto-js": "^4.1.1",
    "@types/jest": "^27.4.0",
    "@types/node": "^18.0.0",
    "@types/sinon": "^10.0.12",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.5",
    "concurrently": "^7.2.2",
    "copy-webpack-plugin": "^11.0.0",
